# PostgreSQL Database Configuration
# Copy this file to .env and update with your actual credentials

# Database Connection
DB_USERNAME=postgres
DB_PASSWORD=your_postgres_password_here

# Alternative database configurations (uncomment to use)
# For custom user:
# DB_USERNAME=faang_user
# DB_PASSWORD=secure_password_123

# For different host/port:
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=examprep
# DB_SCHEMA=sdeexamprep

# Application Configuration
SERVER_PORT=3008
SPRING_PROFILES_ACTIVE=dev

# Security (for production)
# JWT_SECRET=your_jwt_secret_here
# ADMIN_USERNAME=admin
# ADMIN_PASSWORD=secure_admin_password

# Logging Level
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_LEARNINGPORTAL=DEBUG